{
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon -r dotenv/config ./app.mjs",
    "start2": "cross-env PORT=3001 nodemon -r dotenv/config ./app.mjs",
    "fs-start": "cross-env NOTES_MODEL=fs nodemon -r dotenv/config ./app.mjs",
    "level-start": "cross-env NOTES_MODEL=level nodemon -r dotenv/config ./app.mjs",
    "sqlite3-start": "cross-env NOTES_MODEL=sqlite3 nodemon -r dotenv/config ./app.mjs",
    "sqlite3-setup": "sqlite3 chap07.sqlite3 --init models/schema-sqlite3.sql",
    "sequelize-start": "cross-env NOTES_MODEL=sequelize nodemon -r dotenv/config ./app.mjs",
    "mongodb-start": "cross-env NOTES_MODEL=mongodb nodemon -r dotenv/config ./app.mjs"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bootstrap": "4.5.x",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "1.17.x",
    "feather-icons": "4.25.x",
    "fs-extra": "^10.0.0",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "jquery": "3.5.x",
    "js-yaml": "3.13.x",
    "level": "^6.0.1",
    "mongodb": "^3.6.10",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "passport.socketio": "3.7.x",
    "popper.js": "1.16.x",
    "rotating-file-stream": "^2.1.5",
    "sequelize": "^6.6.5",
    "session-file-store": "1.5.0",
    "socket.io": "^2.4.1",
    "sqlite3": "^5.0.2",
    "superagent": "5.2.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12"
  },
  "nodemonConfig": {
    "ignore": [
      "notes-fs-data/*",
      "sessions/*"
    ],
    "ext": "mjs, js, json, env"
  }
}
