  },
  "dependencies": {
    "bootstrap": "4.5.x",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "feather-icons": "4.25.x",
    "fs-extra": "^10.0.0",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "jquery": "3.5.x",
    "level": "^6.0.1",
    "morgan": "~1.9.1",
    "popper.js": "1.16.x",
    "rotating-file-stream": "^2.1.5",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.12"
  },
  "nodemonConfig": {
